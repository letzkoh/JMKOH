<script type="text/javascript">
	
	/* num의 값이 없는 경우 */
	var num;
	
	if (num == 'undefined') {
		alert('첫번째 경우는 undefined가 맞다');
	} else {
		alert('첫번째 경우는 undefined가 아니다');
	}
	
	/* num의 값이 있는 경우 */
	var num = 1;
	
	if (num == 'undefined') {
		alert('두번째 경우는 num 이 1이다');
	} else {
		alert('두번째 경우는 num 이 1아니다');
	}
		
